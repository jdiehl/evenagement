{
  "name": "evenagement",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "tsc --project . && eslint src/",
    "lint": "eslint src",
    "prepare": "husky install",
    "dev": "next dev",
    "dev-emulator": "concurrently -k \"npm:dev\" \"npm:emulator\"",
    "build": "next build",
    "start": "next start",
    "deploy": "firebase deploy",
    "emulator": "firebase emulators:start --import=./emulator-data --export-on-exit",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.4.5",
    "date-fns": "^2.22.1",
    "firebase": "^8.7.0",
    "next": "^11.0.1",
    "phosphor-react": "^1.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.10.1"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.3",
    "@storybook/addon-essentials": "^6.3.3",
    "@storybook/addon-links": "^6.3.3",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.3.3",
    "@types/node": "^15.14.1",
    "autoprefixer": "^10.2.6",
    "concurrently": "^6.2.0",
    "eslint": "^7.30.0",
    "eslint-config-next": "^11.0.1",
    "eslint-plugin-evenagement": "^1.0.0",
    "husky": "^6.0.0",
    "postcss": "^8.3.5",
    "tailwindcss": "^2.2.4",
    "typescript": "^4.3.5"
  }
}
