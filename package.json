    "@typescript-eslint/parser": "^4.20.0",
    "autoprefixer": "^9.8.6",
    "concurrently": "^6.0.0",
    "eslint": "^7.23.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.23.1",
    "husky": "^5.2.0",
    "postcss": "^7.0.35",
    "typescript": "^4.2.3"
  }
