{
  "name": "evenagement",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "tsc --project . && eslint src/",
    "lint": "eslint src",
    "prepare": "husky install",
    "dev": "next dev",
    "dev-emulator": "concurrently -k \"npm:dev\" \"npm:emulator\"",
    "build": "next build",
    "start": "next start",
    "deploy": "firebase deploy",
    "emulator": "firebase emulators:start --import=./emulator-data --export-on-exit",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.4.7",
    "date-fns": "^2.25.0",
    "dayzed": "^3.2.2",
    "firebase": "^9.1.2",
    "next": "^11.1.2",
    "phosphor-react": "^1.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.17.2"
  },
  "devDependencies": {
    "@storybook/addon-actions": "^6.3.11",
    "@storybook/addon-essentials": "^6.3.11",
    "@storybook/addon-links": "^6.3.11",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.3.11",
    "@types/node": "^16.10.4",
    "autoprefixer": "^10.3.7",
    "concurrently": "^6.3.0",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.2",
    "eslint-plugin-evenagement": "^1.0.0",
    "husky": "^7.0.2",
    "postcss": "^8.3.9",
    "tailwindcss": "^2.2.16",
    "typescript": "^4.4.3"
  }
}
