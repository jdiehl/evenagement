{
  "name": "evenagement",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "prepare": "husky install",
    "start": "turbo run start",
    "storybook": "turbo run storybook",
    "test": "turbo run test",
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "emulator": "turbo run emulator --parallel",
    "deploy": "turbo run deploy"
  },
  "devDependencies": {
    "husky": "^7.0.4",
    "turbo": "^1.0.28"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@types/node": "^17.0.2"
  },
  "packageManager": "yarn@1.22.17"
}
