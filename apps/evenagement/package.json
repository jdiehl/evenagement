{
  "name": "evenagement",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "test": "tsc --noEmit && eslint src/",
    "lint": "eslint src",
    "prepare": "husky install",
    "dev": "next dev",
    "dev-emulator": "concurrently -k \"npm:dev\" \"npm:emulator\"",
    "build": "next build",
    "start": "next start",
    "deploy": "firebase deploy",
    "emulator": "firebase emulators:start --import=./emulator-data --export-on-exit",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.6.0",
    "date-fns": "^2.27.0",
    "dayzed": "^3.2.2",
    "firebase": "^9.6.1",
    "next": "^12.0.7",
    "phosphor-react": "^1.3.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.22.1",
    "ui": "*",
    "use-doc-form": "*",
    "user": "*",
    "types": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@storybook/addon-actions": "^6.4.9",
    "@storybook/addon-essentials": "^6.4.9",
    "@storybook/addon-links": "^6.4.9",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.4.9",
    "@types/node": "^16.11.13",
    "autoprefixer": "^10.4.0",
    "concurrently": "^6.4.0",
    "eslintconfig": "*",
    "eslint": "^7.32.0",
    "eslint-config-next": "^11.1.3",
    "eslint-plugin-evenagement": "^1.0.0",
    "husky": "^7.0.4",
    "next-transpile-modules": "9.0.0",
    "postcss": "^8.4.5",
    "tailwindcss": "^2.2.19",
    "tsconfig": "*",
    "typescript": "^4.5.4"
  }
}
